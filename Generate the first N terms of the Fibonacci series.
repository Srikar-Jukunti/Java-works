// You are using Java
import java.util.Scanner;
class main
{
    public static void main(String[]args)
    {
        Scanner s=new Scanner(System.in);
        int t=s.nextInt();
        for(int i=0;i<t;i++)
        {
            int N=s.nextInt();
            int a=0,b=1;
            System.out.println(a+" "+b);
            for(int j=2;j<N;j++)
            {
                int c=a+b;
                System.out.println(" "+c);
                a=b;
                b=c;
            }
            System.out.println();
        }
    }
}
