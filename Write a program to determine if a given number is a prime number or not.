// You are using Java
import java.util.*;
class main
{
    public static void main(String[]args)
    {
        Scanner s=new Scanner(System.in);
        int num=s.nextInt();
        boolean isPrime=true;
        if(num <= 1)
        {
            isPrime=false;
        }
        else
        {
            for(int i=2;i<=Math.sqrt(num);i++)
            {
                if(num%i==0)
                {
                    isPrime=false;
                    break;
                }
            }
        }
         if(isPrime)
         {
             System.out.println(num+" is prime number");
         }
         else
        System.out.println(num+" is not prime number");
    }
}
