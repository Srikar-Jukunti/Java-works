// You are using Java
import java.util.Scanner;
class main
{
    public static void main(String[]args)
    {
        Scanner s=new Scanner(System.in);
        String num=s.nextLine();
        int sum=0;
        for(int i=0;i<num.length();i++)
        {
          sum+=num.charAt(i)-'0';  
        }
        System.out.println(sum);
    }
}
